{
  "name": "unb-libraries/systems-toolkit",
  "description": "Services for developing drupal core and modules",
  "type": "library",
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "aws/aws-sdk-php": "^3",
    "consolidation/robo": "^3",
    "czproject/git-php": "^4",
    "ext-curl": "*",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7",
    "knplabs/github-api": "^3",
    "league/csv": "^9",
    "league/html-to-markdown": "^4",
    "lesstif/php-jira-rest-client": "^1",
    "php": "^8",
    "sebastian/diff": "^4",
    "symfony/cache": "^4",
    "symfony/css-selector": "^5",
    "symfony/dom-crawler": "^4"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Jacob Sanford",
      "email": "jsanford@unb.ca"
    }
  ],
  "autoload": {
    "psr-4": {
      "UnbLibraries\\SystemsToolkit\\": "src"
    }
  },
  "bin": [
    "bin/syskit"
  ]
}
